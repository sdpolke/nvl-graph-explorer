{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend proxy server for Neo4j and OpenAI API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "start": "node dist/server.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "setup:vector-indexes": "ts-node scripts/setup-vector-indexes.ts",
    "check:vector-indexes": "ts-node scripts/check-vector-indexes.ts",
    "test:vector-query": "ts-node scripts/test-vector-query.ts",
    "generate:embeddings": "ts-node scripts/generate-embeddings.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "neo4j-driver": "^6.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "fast-check": "^4.3.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
